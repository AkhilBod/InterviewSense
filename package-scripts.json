{
  "name": "content-generation",
  "scripts": {
    "generate-content": "tsx src/scripts/generate-content.ts",
    "generate-content:dev": "tsx src/scripts/generate-content.ts --output-dir ./generated-content --base-url http://localhost:3000 --sitemap --index",
    "generate-content:prod": "tsx src/scripts/generate-content.ts --output-dir ./generated-content --base-url https://interviewsense.com --sitemap --index --batch-size 25"
  },
  "devDependencies": {
    "tsx": "^4.6.2"
  }
}
